SELECT * FROM persons JOIN contact ON persons.id=contact.persons_id
SELECT persons.name,contact.contact_no FROM persons JOIN contact ON persons.id = contact.persons_id
SELECT persons.name,contact.contact_no FROM persons JOIN contact ON persons.id = contact.persons_id WHERE persons.id=1
SELECT  persons.name,brother.name FROM persons JOIN brother ON persons.id=brother.id
SELECT  persons.name,brother.name FROM persons JOIN brother ON persons.id=brother.id WHERE
brother.persons_id =1
